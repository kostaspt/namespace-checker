{
    "name": "kostaspt/php-ns-fixer",
    "description": "Find wrong namespaces in your PHP projects",
    "type": "application",
    "license": "MIT",
    "keywords": ["namespace"],
    "authors": [
        {
            "name": "Konstantinos Pittas",
            "email": "konstantinos.pittas@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "spatie/regex": "^1.4",
        "symfony/console": "^4.2",
        "symfony/event-dispatcher": "^4.2",
        "symfony/finder": "^4.2",
        "tightenco/collect": ">=5.7.2 <6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "spatie/phpunit-snapshot-assertions": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "PhpNsFixer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpNsFixer\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/php-ns-fixer"
    ],
    "scripts": {
        "test": "phpunit -c phpunit.xml.dist"
    },
    "config": {
        "sort-packages": true
    }
}
